@model UsuarioModel
@{
    ViewData["Title"] = "Usuarios";
} 

<div class="text-center">
    <h3 >Delete Confirmation</h3>
    <form asp-controller="Usuarios" asp-action="Deletar" method="post">
        <p>Are you sure about deleting @Model.NickName usuarios?</p>

        <a class="btn btn-danger" asp-controller="Usuarios" asp-action="Index">Não</a>
        <button type="submit" class="btn btn-primary">Sim</button>
    </form>
</div>
